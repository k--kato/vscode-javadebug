{
  "version": "2.0.0",
  "echoCommand": true,
  "command": "./mvnw",
  "presentation": {
    "echo": true,
    "reveal": "always",
    "focus": false,
    "panel": "shared"
  },
  "tasks": [
    {
      "label": "clean",
      "args": ["clean"],
      "type": "shell"
    },
    {
      "label": "validate",
      "args": ["validate"],
      "type": "shell"
    },
    {
      "label": "compile",
      "args": ["compile"],
      "type": "shell",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "test",
      "args": ["test"],
      "type": "shell",
      "group": {
        "kind": "test",
        "isDefault": true
      }
    },
    {
      "label": "package",
      "args": ["package"],
      "type": "shell"
    },
    {
      "label": "verify",
      "args": ["verify"],
      "type": "shell"
    },
    {
      "label": "install",
      "args": ["install"],
      "type": "shell"
    },
    {
      "label": "jetty:run",
      "args": ["jetty:run"],
      "type": "shell"
    }
  ]
}